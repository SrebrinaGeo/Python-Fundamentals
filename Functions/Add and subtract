def sum_numbers(n1, n2):
    return n1 + n2


def subtract(result, n3):
    return result - n3


def add_and_subtract(n1, n2, n3):
    returned_result = sum_numbers(n1, n2)
    final_result = subtract(returned_result, n3)
    return final_result


num_1 = int(input())
num_2 = int(input())
num_3 = int(input())
print(add_and_subtract(num_1, num_2, num_3))